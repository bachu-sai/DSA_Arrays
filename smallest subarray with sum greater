
def sb(self, a, n, x):
        # Your code goes here 
        i=0
        j=0
        s=0
        mini=1000
        while(i<=j and j<n):
            while(s<=x and j<n):
                s+=a[j]
                j=j+1
            while(s>x and i<j):
                mini=min(mini,j-i)
                s-=a[i]
                i=i+1
        return mini
